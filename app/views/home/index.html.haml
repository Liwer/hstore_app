/ ==============================header=================================
.slider_box
  .slider_box2
    .container_12
      .grid_12
        .slider_wrapper
          #camera_wrap.camera_wrap{:style => "display: block; margin-bottom: 20px; height: 404px;"}
            .camera_fakehover
              .camera_src.camerastarted.hovered
                %div{"data-src" => "/assets/slide1.jpg"}
                %div{"data-src" => "/assets/slide2.jpg"}
                %div{"data-src" => "/assets/slide3.jpg"}
              .camera_target
                .cameraCont
                  .cameraSlide.cameraSlide_0{:style => "visibility: visible; display: none; z-index: 1;"}
                    %img.imgLoaded{"data-alignment" => "", "data-portrait" => "", :height => "404", :src => "/assets/slide1.jpg?1425137453190", :style => "visibility: visible; height: 404px; margin-left: -6.5px; margin-top: 0px; position: absolute; width: 939px;", :width => "939"}/
                    .camerarelative{:style => "width: 926px; height: 404px;"}
                  .cameraSlide.cameraSlide_1{:style => "display: none; z-index: 1;"}
                    %img.imgLoaded{"data-alignment" => "", "data-portrait" => "", :height => "404", :src => "/assets/slide2.jpg?1425137454300", :style => "visibility: visible; height: 404px; margin-top: 0px; position: absolute; margin-left: -6.5px; width: 939px;", :width => "939"}/
                    .camerarelative{:style => "width: 926px; height: 404px;"}
                  .cameraSlide.cameraSlide_2.cameracurrent{:style => "display: block; z-index: 999;"}
                    %img.imgLoaded{"data-alignment" => "", "data-portrait" => "", :height => "404", :src => "/assets/slide3.jpg?1425137466527", :style => "visibility: visible; height: 404px; margin-left: -6.5px; margin-top: 0px; position: absolute; width: 939px;", :width => "939"}/
                    .camerarelative{:style => "width: 926px; height: 404px;"}
                  .cameraSlide.cameraSlide_3.cameranext{:style => "z-index: 1; display: none;"}
                    .camerarelative{:style => "width: 926px; height: 404px;"}
              .camera_overlayer
              .camera_target_content
                .cameraContents
                  .cameraContent{:style => "display: none;"}
                  .cameraContent{:style => "display: none;"}
                  .cameraContent.cameracurrent{:style => "display: block;"}
              .camera_bar{:style => "display: none; top: auto; height: 7px;"}
                %span.camera_bar_cont{:style => "opacity: 0.8; position: absolute; left: 0px; right: 0px; top: 0px; bottom: 0px; background-color: rgb(34, 34, 34);"}
                  %span#pie_0{:style => "opacity: 0.8; position: absolute; left: 0px; right: 0px; top: 2px; bottom: 2px; display: block; background-color: rgb(238, 238, 238);"}
              .camera_commands
                .camera_play{:style => "display: none;"}
                .camera_stop{:style => "display: block;"}
              .camera_prev
                %span
              .camera_next
                %span
            .camera_pag
              %ul.camera_pag_ul
                %li.pag_nav_0{:style => "position:relative; z-index:1002"}
                  %span
                    %span 0
                %li.pag_nav_1{:style => "position:relative; z-index:1002"}
                  %span
                    %span 1
                %li.pag_nav_2.cameracurrent{:style => "position:relative; z-index:1002"}
                  %span
                    %span 2
            .camera_loader{:style => "display: none; visibility: visible;"}
